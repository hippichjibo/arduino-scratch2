{
    "name": "Scratch  Arduino connector",
    "description": "Connect Scratch to Arduino boards",
  "version": "1.0.0",
    "manifest_version": 2,
    "app": {
        "background": {
            "page": "background.html"
        }
    },
    "permissions": [
        "serial"
    ],
    "externally_connectable": {
        "matches": [
            "http://scratchx.org/*"
        ],
        "accepts_tls_channel_id": false
    },
    "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
